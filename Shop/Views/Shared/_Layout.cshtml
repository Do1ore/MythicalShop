<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Shop</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="https://bootswatch.com/5/vapor/bootstrap.css" />
</head>
<body>
    <header style="z-index: 9999" >
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-dark bg-dark border-bottom box-shadow">
            <div class="container">
                <a class="navbar-brand" id="blink5" asp-area=" " asp-controller="Home" asp-action="Index">Мистическая</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav">
                        <li class="nav-item">
                            <a class="nav-link" id="blink6" asp-area="" asp-controller="Home" asp-action="Index">Милый дом</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" id="blink6" asp-area="" asp-controller="Home" asp-action="MyInv">Мои зачерования</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" id="blink6" asp-area="" asp-controller="Home" asp-action="Privacy">Кинуть камень</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" id="blink6" asp-area="" asp-controller="Home" asp-action="Privacy">Воткнуть иглу</a>
                        </li>

                    </ul>
                    <partial name="_LoginPartial" />
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
           &copy; 2022 - Shop - Мистический магазин
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
